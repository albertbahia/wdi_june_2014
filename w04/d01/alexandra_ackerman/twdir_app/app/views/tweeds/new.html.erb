<h2>New Tweed</h2>

<form action="/tweeds" method="post">
  <label>Author:</label>
  <input name="tweed[author]">

  <label>Content:</label>
  <input name="tweed[content]">

  <label>Image URL:</label>
  <input name="tweed[image_url]">

  <label>Timestamp:</label>
  <input type="time" name="tweed[timestamp]">

  <input type="submit" value="Create">
</form>
