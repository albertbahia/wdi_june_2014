<h2>Tweeds</h2>

<a href="/tweeds/new">Add Tweed</a>

<ul>
  <% @tweeds.each do |tweed| %>
    <li>
      <a href="/tweeds/<%= tweed.id %>">
        <%= tweed.author %><br>
        <%= tweed.content %><br>
        <img src="<%= tweed.image_url %>"/><br>
        <%= tweed.created_at %><br>
      </a>
    </li>
  <% end %>
</ul>
