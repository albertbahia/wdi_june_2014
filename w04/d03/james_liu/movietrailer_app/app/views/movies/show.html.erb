<h2><%= @movie.title %></h2>
<h4><%= @movie.year %></h4>
<h3><%= @movie.plot %></h3>

<ul>
  <% @movie.actors.each do |actor| %>
  <li><%= actor.name %></li>
  <% end %>
</ul>

<img src="<%= @movie.poster_url %>">

<%= link_to('Edit Movie', edit_movie_path(@movie) %>
<%= link_to('Back to Movies', movies_path %>
<%= link_to('Delete Movie', movie_path(@movie), method: :delete %>