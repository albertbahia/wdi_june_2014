<h2>New Artist</h2>


<ul>
<% @artist.errors.full_messages.each do |msg| %>
<li><%= msg %></li>
<% end %>
</ul>

<form action="<%= artists_path %>" method="post">
  <label>Name:</label>
  <input name="artist[name]">

  <label>Photo URL:</label>
  <input name="artist[photo_url]">

  <label>Nationality:</label>
  <input name="artist[nationality]">

  <input type="submit" value="Create">
</form>

<%= link_to('Back to Artists', artists_path) %>
