<h2>Add New Trailer</h2>
<ul>
  <% @trailer.errors.full_messages.each do |err_msg| %>
  <li><%= err_msg %></li>
  <% end %>
</ul>

<div id="form">
  <%= form_for @trailer do |f| %>
  Title: <%= f.text_field(:title) %></br>
  Embed url: <%= f.url_field(:embed_url) %>
  <%= f.submit %>
</div>
  <% end %>
