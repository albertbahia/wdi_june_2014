<h2><%= @actor.name %></h2>
<img src="<%= @actor.photo_url %>" alt="" />
<%= link_to("In the movie: #{@actor.movie.title}", movie_path(@actor.movie)) %>

<%= link_to('Back to Actors', actors_path) %>
<%= link_to('Edit', edit_actor_path(@actor)) %>
<%= link_to('Delete', actor_path(@actor), {method: :delete}) %>
