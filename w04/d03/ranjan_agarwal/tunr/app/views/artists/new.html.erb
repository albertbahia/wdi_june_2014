<h2>New Artist</h2>

<ul>
  <% @artist.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
</ul>


<%= form_for(@artist) do |f| %>
  <%= f.label(:name) %>
  <%= f.text_field(:name) %>

  <%= f.label(:photo_url) %>
  <%= f.text_field(:photo_url) %>

  <%= f.label(:nationality) %>
  <%= f.text_field(:nationality) %>

  <%= f.submit %>
<% end %>

<%= link_to('Back to Artists', artists_path) %>
