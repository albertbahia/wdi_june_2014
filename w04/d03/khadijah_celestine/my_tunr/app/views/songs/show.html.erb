<h2 ><%= @song.title %></h2>
<h3>By: <%= @song.artist.name %></h3>

Album: <%= @song.album %></br>
Preview_url: <%= @song.preview_url %></br>
<audio controls>
  <source src="<%= @song.preview_url %>" type="audio/mpeg">
</audio> <br />
Price: <%= @song.price %></br>

<%= link_to('Edit', edit_song_path(@song)) %>
<%= link_to('Delete', song_path(@song), method: :delete) %>
