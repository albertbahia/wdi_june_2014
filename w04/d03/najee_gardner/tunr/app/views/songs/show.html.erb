<h2><%= @song.title %></h2>
<h3>By: <%= link_to @song.artist.name, artist_path(@song.artist) %></h3>

Album: <%= @song.album %></br>
Preview_url: <%= @song.preview_url %></br>
Price: <%= @song.price %></br>

<%= link_to 'Edit', edit_song_path(@song) %>
<%= link_to 'Delete', song_path(@song), method: :delete %>
