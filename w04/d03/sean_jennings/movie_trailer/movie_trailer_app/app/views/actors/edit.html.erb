<%= link_to 'Back to Actor', actor_path(@actor) %>

<%= form_for @actor do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :photo_url %>
    <%= f.text_field :photo_url %>
  </div>

    <%= f.submit %>

<% end %>

<%= button_to "Delete", actor_path(@actor), method: :delete %>
