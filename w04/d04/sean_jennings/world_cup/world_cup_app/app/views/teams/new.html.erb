<p>Add a Team</p>

<%= form_for @team do |f| %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <br>
  <%= f.label :flag_url %>
  <%= f.text_field :flag_url %>
  <br>
  <%= f.label :manager_name %>
  <%= f.text_field :manager_name %>
  <br>

  <% f.submit %>
<% end %>
<br>

<%= link_to "Return to Teams", teams_path %>
