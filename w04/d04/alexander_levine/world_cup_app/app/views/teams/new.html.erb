<h2>New Team</h2>

<%= form_for @team do |f| %>
<div>
  <%= f.label :country %>
  <%= f.text_field :country %>
</div>
<div>
  <%= f.label :flag_url %>
  <%= f.text_area :flag_url %>
</div>
<div>
  <%= f.label :manager %>
  <%= f.text_field :manager %>
</div>
<div>
  <%= f.label :group %>
  <%= collection_select(:team, :group_id, Group.all, :id, :name) %>
</div>
<div>
  <%= f.submit %>
</div>
<% end %>
