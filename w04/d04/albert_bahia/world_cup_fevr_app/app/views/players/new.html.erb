<h2>Add Player</h2>

<%= form_for @player do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.label :position %>
  <%= f.text_field :position %>

  <%= f.label :skill_level %>
  <%= f.text_field :skill_level %>

  <%= f.submit("Add Player") %>
<% end %>
