<h2>New Actor</h2>

<ul>
  <% @actor.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
</ul>

<%= form_for(@actor) do |f| %>
  <%= f.label(:name) %>
  <%= f.text_field(:name) %>

  <%= f.label(:photo_url) %>
  <%= f.text_field(:photo_url) %>

  <%= f.submit %>
<% end %>

<%= link_to('Back to All Actors', actors_path) %>
