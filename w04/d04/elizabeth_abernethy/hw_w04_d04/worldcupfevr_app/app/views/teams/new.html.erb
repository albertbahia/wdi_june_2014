<h2>New Team</h2>

<ul>
  <% @team.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
</ul>

<%= form_for(@team) do |f| %>
  <%= f.label(:name) %>
  <%= f.text_field(:name) %>

  <%= f.label(:photo_url) %>
  <%= f.text_field(:photo_url) %>

  <%= f.label(:position) %>
  <%= f.text_field(:position) %>

  <%= f.label(:skill_level) %>
  <%= f.text_field(:skill_level) %>

  <%= f.submit %>
<% end %>

<%= link_to('Back to All Teams', teams_path) %>
