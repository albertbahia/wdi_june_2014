<h2><%= "#{@team.country} | #{@team.group.name} "%></h2>

<div>
	<%= link_to('Back to Teams', teams_path)%>
	<%= link_to('Edit', edit_team_path(@team))%>
</div>

<h3><%= @team.country %></h3>
<img src="<%= @team.flag_url%>">

<ul>
	<% @team.players.each do |player| %>
		<li><%= link_to(player.name, player_path(player)) %></li>
	<% end %>
</ul>
