<h2><%= @team.country %></h2>
<h3>Team <%= @team.country %> Players: </h3>

<p>
  <% @team.players.each do |player| %>
  <%= link_to(player.name, player_path(player)) %> <br>
  <% end %>
</p>

<div class="menu_buttons">
  <%= link_to('Back to Teams', teams_path) %>
  <%= link_to('Edit Team', edit_team_path(@team))%>
</div>
