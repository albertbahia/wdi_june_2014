<h2>New Tweed</h2>

<%= form_for @tweed do |f| %>
  <%= f.label(:author) %>
  <%= f.text_field(:author) %>

  <%= f.label(:content) %>
  <%= f.text_field(:content) %>

  <%= f.label(:image_url) %>
  <%= f.text_field(:image_url) %>

  <%= f.submit %>
<% end %><br>

<%= link_to('Back to TweedFeed', tweeds_path(@tweed)) %>
