<h2>Create New Painting:</h2>

<%= form_for(@painting) do |f| %>
<%= f.label(:img_url) %>
<%= f.text_field(:img_url) %>

<%= f.label(:title) %>
<%= f.text_field(:title) %>

<%= f.label(:year_painted) %>
<%= f.text_field(:year_painted) %>

<%= f.label(:description) %>
<%= f.text_field(:description) %>

<%= f.label(:artist_id) %>
<%= f.text_field(:artist_id) %>

<%= f.submit %>

<% end %>

<p><%= link_to("Back To All Artist", artists_path)%></p>
