<h2>Edit Tweed</h2>

<%= form_for(@tweed) do |f| %>
  <%= f.label(:author) %>
  <%= f.text_field(:author) %>

  <%= f.label(:content) %>
  <%= f.text_field(:content) %>

  <%= f.label(:image_url) %>
  <%= f.text_field(:image_url) %>

  <%= f.submit %>

<% end %>
</br>

<%= link_to('Back to Tweed', tweed_path(@tweed))%>
<%= link_to('Delete', tweed_path(@tweed), method: :delete)%>
