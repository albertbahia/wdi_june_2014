<h2>New Tweed</h2>

<form action="<%= tweeds_path %>" method="post">
  <label>Author:</label>
  <input name="tweed[author]">

  <label>Content:</label>
  <input name="tweed[content]">

  <label>Image URL:</label>
  <input name="tweed[image_url]">

  <input type="submit" value="Create">
</form>

<%= link_to('Back to Tweeds', tweeds_path) %>
