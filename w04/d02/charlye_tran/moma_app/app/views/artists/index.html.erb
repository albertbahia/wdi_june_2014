<h2>Artists</h2>
<p><%= link_to('Moma App', root_path) %></p>

<%= link_to('Add Artist', new_artist_path)%>

<ul>
  <% @artists.each do |artist| %>
  <li>
    <h1><%= link_to(artist.name, artist_path(artist)) %></h1><br>
    <% artist.paintings.each do |painting|%>
      <%= link_to(painting.title, painting_path(painting))%><br>
      <img src="<%=painting.img_url%>" height="400" width="450"><br>
    <% end %>
  </li>
  <% end %>
</ul>
