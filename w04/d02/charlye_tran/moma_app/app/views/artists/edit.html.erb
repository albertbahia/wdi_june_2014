<%= form_for(@artist) do |f| %>
  <%= f.label(:name)%>
  <%= f.text_field(:name) %>

  <%= f.label(:photo_url)%>
  <%= f.text_field(:photo_url)%>

  <%= f.label(:nationality)%>
  <%= f.text_field(:nationality)%>

  <%= f.label(:birthday) %>
  <%= f.text_field(:birthday) %>

  <%= f.submit %>
<% end %>
<br>

<%= link_to("Back to #{@artist.name}", artist_path(@artist)) %><br>
<%= link_to("Delete #{@artist.name}", artist_path(@artist), { method: :delete })%>
<% end %>
