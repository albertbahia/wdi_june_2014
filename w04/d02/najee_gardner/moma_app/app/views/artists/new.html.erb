<h3>Create New Artist</h3>

<%= form_for(@artist) do |f| %>
  <%= f.label(:name) %>
  <%= f.text_field(:name) %>

  <%= f.label(:photo_url, 'Photo URL') %>
  <%= f.text_field(:photo_url) %>

  <%= f.label(:nationality) %>
  <%= f.text_field(:nationality) %>

  <%= f.label(:birthday) %>
  <%= f.text_field(:birthday) %>

  <%= f.submit %>

<% end %>

<%= link_to 'Back to All Artist', artists_path %>
