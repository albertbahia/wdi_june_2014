<h3>Create New Artist</h3>

<%= form_for(@artist) do |f| %>
  <%= f.label(:img_url) %>
  <%= f.text_field(:img_url) %>

  <%= f.label(:title) %>
  <%= f.text_field(:title) %>

  <%= f.label(:year_painted) %>
  <%= f.text_field(:year_painted) %>

  <%= f.label(:description) %>
  <%= f.text_field(:description) %>

  <%= f.submit %>

<%= end %>

<%= link_to 'Back to All Artist', artists_path %>
