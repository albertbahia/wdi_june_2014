<h2><%= @artist.name %></h2>
<h3><%= @artist.nationality %></h3>
<img src="<%= @artist.photo_url %>">

<ul>
  <% @artist.songs.each do |song| %>
    <li><%= link_to song.title, song_path(song) %></li>
  <% end %>
</ul>

<%= link_to 'Edit', edit_artist_path %>
<%= link_to 'Delete', artist_path, method: :delete %>
