<h2><%= @artist.name %></h2>
<h3><%= @artist.nationality %></h3>

<ul>
  <% @artist.songs.each do |song| %>
    <li><%= song.title %></li>
  <% end %>
</ul>

<%= link_to('Back to Artists', artists_path) %>
<%= link_to('Edit', edit_artist_path(@artist)) %>
<%= link_to('Delete', artist_path(@artist), {method: :delete }) %>

<img src="<%= @artist.photo_url %>">
