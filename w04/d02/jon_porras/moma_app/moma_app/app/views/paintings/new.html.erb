<h3>NEW PAINTING</h3>

<%= form_for(@painting) do |f| %>
  <%= f.label(:img_url) %>
  <%= f.text_field(:img_url) %>

  <%= f.label(:title) %>
  <%= f.text_field(:title) %>

  <%= f.label(:year_painted) %>
  <%= f.text_field(:year_painted) %>

  <%= f.label(:description) %>
  <%= f.text_field(:description) %>

  <%= f.label(:artist) %>
  <%= f.collection_select :artist_id, Artist.all, :id, :name %>

<%= f.submit %>
<% end %>
