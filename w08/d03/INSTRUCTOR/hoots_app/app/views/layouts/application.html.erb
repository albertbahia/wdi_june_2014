<!DOCTYPE html>
<html>
<head>
  <title>HootsApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <h1>Hoots</h1>
  <div id="hoots">
    <% @hoots.each do |hoot| %>
      <%= render partial: 'hoots/hoot', locals: {hoot: hoot} %>
    <% end %>
  </div>
</body>
</html>
