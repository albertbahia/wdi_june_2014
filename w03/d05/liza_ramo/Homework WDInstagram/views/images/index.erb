<h2>Images</h2>

<ul>
  <% @images.each do |image| %>
    <li>
      <a href="/images/<%= image.id %>">
        <%= image.author %> - <%= image.date_posted %> - <%= image.caption %>
      </a></br>
      <img src= "<%= image.image_url %>"></br>
    </li>
  <% end %>
</ul>

<a href="/images/new">Add Image</a>
