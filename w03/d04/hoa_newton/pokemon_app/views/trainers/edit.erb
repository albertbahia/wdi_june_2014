<h2>Edit Trainer</h2>

<form action="/trainers/<%= @trainer.id %>" method="post">
  <label for="trainer[name]">Name:</label>
  <input type="text" name="trainer[name]" value="<%= @trainer.name %>">

  <label for="trainer[age]">Age:</label>
  <input type="text" name="trainer[age]">

  <label for="trainer[hometown]">Hometown:</label>
  <input type="text" name="trainer[hometown]">

  <input type="submit" value="Edit">
</form>
