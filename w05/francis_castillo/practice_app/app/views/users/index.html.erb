<h2>Users</h2>
<hr>
<% @users.each do |user| %>
  <%= user.id %><br>
  <b><%= user.username %></b><br>
  <%= "#{user.first_name} #{user.last_name}" %><br>
  <%= user.password_digest %><br>
  <% if session[:current_user] == user.id %>
    <%= link_to 'Edit', edit_user_path(user) %>
    <%= link_to 'Remove', user_path(user), method: :delete %>
  <% end %>
  <hr>
<% end %>
