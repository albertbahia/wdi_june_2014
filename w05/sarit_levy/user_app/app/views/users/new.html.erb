<h2> New User</h2>

<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.textfield :username %>
<br>

<%= f.label :first_name %>
<%= f.textfield :first_name %>
<br>

<%= f.label :last_name %>
<%= f.textfield :last_name %>
<br>

<%= f.label :password %>
<%= f.password_field :password %>
<br>

<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>

<%=f.submit %>

<% end %>
