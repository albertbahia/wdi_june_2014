<h2>Edit User: <%= @user.username %></h2>

<%= form_for(@user) do |f| %>
  <%= f.label(:username) %>
  <%= f.text_field(:username) %>

  <%= f.label(:first_name) %>
  <%= f.text_field(:first_name) %>

  <%= f.label(:last_name) %>
  <%= f.text_field(:last_name) %>

  <%= f.submit 'Update'%>
<% end %>
