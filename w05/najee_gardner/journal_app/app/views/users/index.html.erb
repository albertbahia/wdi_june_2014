<h2>Users</h2>
<hr>
<% @users.each do |user| %>
<div>
  <p><%= user.first_name %> <%= user.last_name %></p>
  <p>Username: <%= user.username %></p>
  <p>Password Digest: <%= user.password_digest %>
  <% if session[:current_user] == user.id %>
    <%= link_to 'Edit', edit_user_path(user) %>
    <%= link_to 'Remove', users_path(user), method: :delete %>
  <% end %>
  </div>
<hr>
<% end %>
