<h2>Users</h2>

<% @users.each do |user| %>
<p><%= user.username %></p>
<p><%= user.first_name %></p>
<p><%= user.password_digest %></p>
  <% if session[:current_user] == user.id %>
<p><%= link_to('Delete', user_path(user), method: :delete) %></p>
<p><%= link_to('Edit', edit_user_path(user)) %></p>
  <% end %>

<% end %>



<h6><%= link_to('Create New User', new_user_path(@user)) %></h6>
